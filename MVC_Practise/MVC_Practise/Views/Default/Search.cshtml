@model IEnumerable<MVC_Practise.Models.Student>

@{
    ViewBag.Title = "Search";
}

<h2>Search</h2>

<form>
    <input type="search" name="Score" class="form-control" />
    <input type="submit" class="form-control" />
</form>
<table class="table table-hover">
    <tr>
        <th>Name</th>
        <th>Score</th>
        <th>Birth</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.Score</td>
            <td>@item.Birth</td>
        </tr>
    }
</table>