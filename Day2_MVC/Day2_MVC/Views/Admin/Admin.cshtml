@*model IEnumerable<Day2_MVC.Models.Human>*@

@{
    ViewBag.Title = "Admin";
}

<h2>Admin</h2>

@Html.Partial("GetNames") <!--HTML helper-->

<table class="table table-hover">
    <tr>
        <th>Name</th>
        <th>Age</th>
    </tr>
    @foreach (var item in Model.Humen)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.Age</td>
        </tr>
    }
</table>

<table class="table table-hover">
    <tr>
        <th>Name</th>
        <th>Year</th>
    </tr>
    @foreach (var item in Model.Cars)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.Year</td>
        </tr>
    }
</table>